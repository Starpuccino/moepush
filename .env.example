AUTH_SECRET=
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=
DISABLE_REGISTER=false

# 推送超时设置（毫秒）
# 单接口推送默认超时时间，可通过 X-Timeout Header 覆盖
# 默认值: 10000 (10秒)
PUSH_TIMEOUT=10000

# 接口组并发控制
# 接口组推送时的最大并发数（根据服务器性能调整）
# 默认值: 4
PUSH_GROUP_CONCURRENCY=4

# 回调超时配置
# 异步回调请求的超时时间（毫秒），可通过 X-Callback-Timeout Header 覆盖
# 默认值: 10000 (10秒)
CALLBACK_TIMEOUT=10000

# 日志配置
# 日志级别: DEBUG, INFO, WARN, ERROR（不区分大小写）
# 默认值: DEBUG
LOG_LEVEL=DEBUG

# 日志目录
# 默认值: ./logs
LOG_DIR=./logs